        "\n",
        "# 결측 데이터 0으로 대체\n",
        "df['views'] = df['views'].fillna(0)\n",
        "\n",
        "# views 컬럼에서 10번째로 큰 값을 구합니다.\n",
        "value = df['views'].nlargest(10).iloc[-1]\n",
        "\n",
        "# views 컬럼에서 가장 큰 10개의 값을 10번째로 큰 값으로 대체\n",
        "df.loc[df['views'] > value, 'views'] = value\n",
        "\n",
        "# views 컬럼의 합\n",
        "print(int(df['views'].sum()))"
