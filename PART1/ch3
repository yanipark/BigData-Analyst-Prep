        "\n",
        "# 결측치 제거\n",
        "df = df.dropna()\n",
        "\n",
        "# 새로운 컬럼 계산\n",
        "df['new'] = df['views']/df['f1']\n",
        "\n",
        "# 내림차순 정렬\n",
        "df = df.sort_values('new', ascending=False)\n",
        "\n",
        "# age 값 찾아서 출력\n",
        "int(df.iloc[0,1])"
